@inherits UmbracoViewPage<ACTFoundation.Core.ViewModels.Partials.Layout.PartnerLogosViewModel>

<div style ="display: flex; flex-wrap: wrap; justify-content: space-around; padding: 30px var(--default-spacing);">
    @foreach (var item in Model.PartnerLogos)
    {
        <div style=" width: 100%; height: 100%; object-fit: contain; margin: 10px 8px; width: 135px; height: 75px; overflow: hidden;">
            <a href="#" target="_blank">
                <img src="@item.Url()" alt="ACT Foundation partner logo" />
            </a>
        </div>
    }
</div>