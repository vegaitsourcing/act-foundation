@inherits UmbracoViewPage<HomeViewModel>

@if (Model.BannerCarousel != null)
{
    Html.RenderPartial("~/Views/Partials/NestedContent/_BannerCarousel.cshtml", Model.BannerCarousel);
}
   
@if (Model.DonateBlock != null)
{
    Html.RenderPartial("~/Views/Partials/Blocks/_DonateBlock.cshtml", Model.DonateBlock );
}

@if (Model.Testimonials != null)
{
    Html.RenderPartial("~/Views/Partials/NestedContent/_Testimonials.cshtml", Model.Testimonials);
}

@if (Model.Volunteers != null)
{
    Html.RenderPartial("~/Views/Partials/NestedContent/_Volunteers.cshtml", Model.Volunteers);
}
